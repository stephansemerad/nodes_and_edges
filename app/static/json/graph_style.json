[
  {
    "selector": "node",
    "css": {
      "content": "data(full_name)",
      "color": "#FFFFFF",
      "backgroundColor": "#4f46e5",
      "text-wrap": "wrap",
      "text-max-width": 120,
      "font-size": "15px",
      "text-valign": "center",
      "shape": "rectangle",
      "width": "150px",
      "height": "30px",
      "padding": 2
    }
  },
  {
    "selector": "edge",
    "css": {
      "target-arrow-shape": "triangle",
      "curve-style": "unbundled-bezier",
      "width": 2,
      "line-color": "#888888",
      "target-arrow-color": "#888888"
    }
  },
  {
    "selector": "edge[label]",
    "css": {
      "label": "data(label)",
      "edge-text-rotation": "autorotate",
      "font-size": "16px",
      "color": "#888888"
    }
  }
]
